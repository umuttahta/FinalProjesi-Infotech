<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - ChocolateAppClient</title>
    <link rel="stylesheet" href="~/ui/js/bootstrap.bundle.min.js" />
    <link rel="stylesheet" href="~ui/css/st" asp-append-version="true" />
    <link rel="stylesheet" href="~/ChocolateAppClient.styles.css" asp-append-version="true" />
   

</head>

<body>
    @await Html.PartialAsync("_PreLoaderPartial")


    @await Html.PartialAsync("_HeaderPartial")

    @RenderBody()

    @await Html.PartialAsync("_FooterPartial")
   

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
   
    


    @await RenderSectionAsync("Scripts", required: false)

</body>
</html>
